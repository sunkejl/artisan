{
    "manifest_version": 2,
    "name": "我的时钟1",
    "version": "1.0",
    "description": "我的第一个Chrome扩展",
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon19.png",
            "38": "images/icon38.png"
        },
        "default_title": "我的时钟1",
        "default_popup": "popup.html"
    },
	"content_scripts": [
    {
      "matches": ["https://www.baidu.com/*","https://www.liaoxuefeng.com/*","https://bbs.hupu.com/*","https://www.zhihu.com/*"],
      "js": ["myscript.js"]
    }
  ]
}
